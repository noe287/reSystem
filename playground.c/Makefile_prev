vpath %.c src:txt
# VPATH = src
SUBDIRS = src


routes: routes.c
	gcc -o bin/$@ $^

$(SUBDIRS):
	$(MAKE) -C $@

.PHONY: cleanall cleanobj cleandiff subdirs $(SUBDIRS)

cleanall: cleanobj cleandiff
	rm bin/routes

