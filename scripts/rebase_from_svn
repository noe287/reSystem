<td class='lines'><pre><code>#!/bin/bash

red=`tput setaf 1`
green=`tput setaf 2`
yellow=`tput setaf 3`
blue=`tput setaf 4`
magenda=`tput setaf 5`
cyan=`tput setaf 6`
reset=`tput sgr0`
bold=`tput bold`
underline=`tput smul`

echo
echo &quot;${cyan}${bold}===========================================&quot;
echo &quot;	Rebase Profile&quot;
echo &quot;	Written by Kivanc &amp; Bekir&quot;
echo &quot;===========================================${reset}&quot;
echo

if [ $# -eq 3 ]
then
	echo
	echo &quot;${yellow}${bold}Initializing...${reset}&quot;
else
    echo &quot;${red}${bold}invalid argument please pass only 3 arguments&quot;
    echo &quot;Usage:&quot;
	echo &quot;mv_files.sh Air4920 /path/buildsys_dir/ /path/svn_dir/${reset}&quot;
	echo
	exit
fi

product=$1
buildsys=$2
svn=$3
profiles=$buildsys&quot;profiles/&quot;$product&quot;/&quot;

if [[ &quot;$product&quot; != *&quot;Air4920&quot;* ]];then
	echo
	echo &quot;${red}${bold}Only 4920 product family is supported for now...&quot;
	echo &quot;Exitting...${reset}&quot;
	echo
	exit 0
fi

echo
echo &quot;${blue}${bold}${underline}product:${reset} ${cyan}&quot;$product${reset}
echo &quot;${blue}${bold}${underline}buildsys:${reset} ${cyan}&quot;$buildsys${reset}
echo &quot;${blue}${bold}${underline}svn:${reset} ${cyan}&quot;$svn${reset}
echo &quot;${blue}${bold}${underline}profiles:${reset} ${cyan}&quot;$profiles${reset}
echo

busybox=$(ls -lt $svn | grep busybox.config)
config=$(ls $svn -lt | grep .config | grep -v kernel | grep -v busybox | grep -v xml)
conf_xml=$(ls $svn -lt | grep config.xml)
io_xml=$(ls $svn -lt | grep io.xml)
kernel=$(ls $svn -lt | grep kernel.config)
bootl=$(ls $svn -lt | grep cfe)

busybox=$(echo ${busybox##* })
config=$(echo ${config##* })
io_xml=$(echo ${io_xml##* })
kernel=$(echo ${kernel##* })
bootloader=$(echo ${bootl##* })
config_xml=$(echo ${conf_xml##* })

if [[ -z &quot;$busybox&quot; ]];then
	echo &quot;${red}${bold}busybox config could not been found!!!&quot;
	echo &quot;Exitting...${reset}&quot;
	exit 0
fi

if [[ -z &quot;$config&quot; ]];then
	echo &quot;${red}${bold}profile config could not been found!!!&quot;
	echo &quot;Exitting...${reset}&quot;
	exit 0
fi

if [[ -z &quot;$kernel&quot; ]];then
	echo &quot;${red}${bold}kernel config could not been found!!!&quot;
	echo &quot;Exitting...${reset}&quot;
	exit 0
fi

if [[ -z &quot;$config_xml&quot; ]];then
	echo &quot;${red}${bold}config.xml could not been found!!!&quot;
	echo &quot;Exitting...${reset}&quot;
	exit 0
fi

if [[ -z &quot;$io_xml&quot; ]];then
	echo &quot;${red}${bold}io.xml could not been found!!!&quot;
	echo &quot;Exitting...${reset}&quot;
	exit 0
fi

if [[ -z &quot;$bootloader&quot; ]];then
	echo &quot;${red}${bold}bootloader could not been found!!!&quot;
	echo &quot;Exitting...${reset}&quot;
	exit 0
fi

echo &quot;${yellow}${bold}Copying profile.config...${reset}&quot;
cmd=&quot;cp &quot;$svn&quot;&quot;$config&quot; &quot;$profiles&quot;profile.config&quot;
echo $cmd
eval $cmd
echo &quot;${green}${bold}Done${reset}&quot;

echo
echo &quot;${yellow}${bold}Copying .config...${reset}&quot;
cmd=&quot;cp &quot;$svn&quot;&quot;$config&quot; &quot;$buildsys&quot;.config&quot;
echo $cmd
eval $cmd
echo &quot;${green}${bold}Done${reset}&quot;

echo
echo &quot;${yellow}${bold}Copying kernel.config...${reset}&quot;
cmd=&quot;cp &quot;$svn&quot;&quot;$kernel&quot; &quot;$profiles&quot;kernel.config&quot;
echo $cmd
eval $cmd
echo &quot;${green}${bold}Done${reset}&quot;

echo
echo &quot;${yellow}${bold}Copying busybox.config...${reset}&quot;
cmd=&quot;cp &quot;$svn&quot;&quot;$busybox&quot; &quot;$profiles&quot;busybox.config&quot;
echo $cmd
eval $cmd
echo &quot;${green}${bold}Done${reset}&quot;

echo
echo &quot;${yellow}${bold}Copying config.xml...${reset}&quot;
cmd=&quot;cp &quot;$svn&quot;&quot;$config_xml&quot; &quot;$profiles&quot;config.xml&quot;
echo $cmd
eval $cmd
echo &quot;${green}${bold}Done${reset}&quot;

echo
echo &quot;${yellow}${bold}Copying io.xml...${reset}&quot;
cmd=&quot;cp &quot;$svn&quot;&quot;$io_xml&quot; &quot;$profiles&quot;io.xml&quot;
echo $cmd
eval $cmd
echo &quot;${green}${bold}Done${reset}&quot;

echo
echo &quot;${yellow}${bold}Copying bootloader.bin...${reset}&quot;
cmd=&quot;cp &quot;$svn&quot;&quot;$bootloader&quot; &quot;$profiles&quot;bootloader.bin&quot;
echo $cmd
eval $cmd
echo &quot;${green}${bold}Done${reset}&quot;

echo
echo &quot;${red}${bold}======================================================================&quot;
echo &quot;Please don't forget to check ${underline}filesystem${reset}${red}${bold} folder and ${underline}preinstall${reset}${red}${bold} file!!!&quot;
echo &quot;======================================================================${reset}&quot;
echo
</code></pre></td></tr></table>
